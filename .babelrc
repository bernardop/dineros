{
  "presets": [
    "react",
    "es2015",
    "es2016",
    "stage-1"
  ],
  "env": {
    "start": {
      "plugins": [
        ["react-transform", {
          "transforms": [
            {
              "transform": "react-transform-hmr",
              "imports": ["react"],
              "locals": ["module"]
            }
          ]
        }]
      ]
    }
  },
  "plugins": [
    "transform-flow-strip-types",
    "transform-decorators-legacy",
    "lodash"
  ]
}
